import os

def main():
    p = []
    new = []
    res2 = []
    p = os.popen("ovs-vsctl get Open_vSwitch . other_config")
    for line in p.readlines():
        result = str(line).split("0x")[1].split("\"")[0]
        hexa = int(result, 16)
        binar = bin(hexa).split("0b")[1]
        for i in range(len(binar)-1,-1,-1):
                new.append(binar[i])
    for k in range (0,len(new)):
        if new[k] == "1":
                res2.append(k)
    return res2
print main ()
